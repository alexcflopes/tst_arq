    -e ARCHITECTURES_REPOSITORY_URI=https://github.com/alexcflopes/tst_arq.git \
    -e ARCHITECTURES_REPOSITORY_BRANCH=main \
    -e ARCHITECTURES_REPOSITORY_PATH=arqs \
    -p 8080:8080 \
    mbouchenoire/lowfer:latest
